    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.0.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/archiver": "^7.0.0",
    "@types/node": "^20.19.30",
    "@types/nodemailer": "^7.0.5",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "apns2": "^12.2.0",
    "archiver": "^7.0.1",
    "blurhash": "^2.0.5",
    "bullmq": "^5.0.0",
